{
  "env": {
    "node": true,
    "jest": true,
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": "off",
    "func-names": "off",
    "camelcase": "off"
  }
}
